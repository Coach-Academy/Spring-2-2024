#include<bits/stdc++.h>
using namespace std;
#define ll long long
signed main()
{
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int n;cin>>n;
    int s[n];
    for(auto&it:s)
    {
        cin>>it;
    }
    int mx=s[0],mn=s[0],ans=0;
    for(int i=1;i<n;i++)
    {
        if(s[i]>mx||s[i]<mn)
        {
            ans++;
        }
        mx=max(mx,s[i]);
        mn=min(mn,s[i]);
    }
    cout<<ans;
    return 0;
}
